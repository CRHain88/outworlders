{
  "require": {
    "craftcms/cms": "^3.0.0",
    "craftcms/redactor": "2.3.2",
    "dolphiq/redirect": "1.0.20",
    "dolphiq/sitemap": "1.0.9",
    "ether/seo": "3.5.4",
    "newism/craft3-fields": "0.0.14",
    "nystudio107/craft-seomatic": "3.1.46",
    "topshelfcraft/supersort": "3.0.1",
    "verbb/feed-me": "3.1.16",
    "vlucas/phpdotenv": "^2.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
